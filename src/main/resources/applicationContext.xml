<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Example bean definition -->

    <bean id="emailService" class="com.revature.ioc.EmailService"/>
    <bean id="smsService" class="com.revature.ioc.SmsService"/>

    <bean id="messageSender" class="com.revature.ioc.MessageSender" init-method="initmethod" destroy-method="destroymethod">
        <property name="messageService" ref="emailService"/>
    </bean>

</beans>